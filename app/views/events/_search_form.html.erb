<%= form_tag events_path, method: :get do %>
  <%= text_field_tag :query,
    params[:query],
    class: "form-control",
    placeholder: "Find an event"%>
  <%= submit_tag "SURPRISE ME", class: "btn btn-primary" %>
    placeholder: "Find an event/ Postcode"%>
  <%= submit_tag "Search", class: "btn btn-primary" %>
<% end %>
