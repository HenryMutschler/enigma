<div class="container">
  <h1></h1>
  <div><%= image_tag "#{current_user.picture_url}", id: "avatar-center" %>
  </div>
  <div>
    <%= link_to "⭐️⭐️⭐️⭐️⭐️", user_path %><br>
    <%= link_to "✎ Edit Profile", edit_user_registration_path %><br>
    <%= link_to "⫸ Sign Out", destroy_user_session_path, method: :delete %><br>
  </div>
  <br>
  <div>
      <h4>See what people said about<br>your events:</h4>
    <ul class="list">
      <% current_user.reviews.by_recently_created.each do |review| %>
        <li class="list-item">
        <div class="list-content"><%= review.booking.event.event_name %><br>
          <%= review.booking.event.start_time.strftime('%d %b %Y') %> |
          <%= review.booking.user.first_name %></div>
        <p>_________________________</p>
        <%= review.content %></li>
      <% end %>
    </ul>
  </div>
</div>
